<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.no0ne.appointmentsystem">

    <application
        android:allowBackup="true"
        android:icon="@drawable/calendar_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogInActivity"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SignInActivity"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TeacherActivity"
            android:label="Teacher"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StudentActivity"
            android:label="Student"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TeacherInfoActivity"
            android:label="Edit Info"
            android:parentActivityName=".TeacherActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StudentInfo"
            android:label="Edit Info"
            android:parentActivityName=".StudentActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>